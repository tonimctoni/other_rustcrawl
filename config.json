{
    "initial_urls": ["https://www.wikipedia.org/"],
    "report_period": 1000,
    "enable_https":true,
    "client_threads":4,
    "dns_threads":4,
    "request_timeout":2000,
    "max_body_len":32000000,
    "client_buffer_size":40,
    "max_urls_per_html":200,
    "max_urls_per_host_per_html":50,
