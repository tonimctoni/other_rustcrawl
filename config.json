        "max_size":32000000
    },
    {
        "mimetype":"image/gif",
        "file_prefix":"gif_image",
        "file_extension":"gif",
        "folder_name":".output",
        "min_size":65536,
        "max_size":32000000
    },
    {
        "mimetype":"image/jpeg",
        "file_prefix":"jpeg_image",
        "file_extension":"jpeg",
        "folder_name":".output",
        "min_size":65536,
        "max_size":32000000
    },
    {
        "mimetype":"image/tiff",
        "file_prefix":"tiff_image",
        "file_extension":"tiff",
        "folder_name":".output",
        "min_size":65536,
        "max_size":32000000
    },
    {
        "mimetype":"image/bmp",
        "file_prefix":"bmp_image",
        "file_extension":"bmp",
        "folder_name":".output",
        "min_size":65536,
        "max_size":32000000
